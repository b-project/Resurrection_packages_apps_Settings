<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/about_bluros_settings_title"
	xmlns:bluros="http://schemas.android.com/apk/res/com.android.settings">
                
 	<PreferenceCategory
		android:key="links"
		android:title="@string/rr"> 
             
		<Preference
			android:key="bluros_website"
			android:title="@string/bluros_website_title"
			android:summary="@string/bluros_website_summary" />
			
		<Preference
			android:key="bluros_forum"
			android:title="@string/bluros_forum_title"
			android:summary="@string/bluros_forum_summary" />

		<Preference
			android:key="bluros_source"
			android:title="@string/bluros_source_title"
			android:summary="@string/bluros_source_summary" />


		<Preference
			android:key="bluros_facebook"
			android:title="@string/bluros_facebook_title"
			android:summary="@string/bluros_facebook_summary" />
			
		<Preference
			android:key="bluros_donate"
			android:title="@string/bluros_donate"
			android:summary="@string/bluros_donate_summary" />
                			
</PreferenceCategory>
				
	<PreferenceCategory
		android:key="devs"
		android:title="@string/dev_categorie_title">

		<com.android.settings.widgets.DeveloperPreference
            android:title="bluros_"
            bluros:nameDev="BlurOS Team"
            bluros:gplusHandle="BlurOS"
            bluros:donateLink="@string/github_bluros_"
            bluros:emailDev="xdavn.vn@gmail.com"/>

</PreferenceCategory>             

</PreferenceScreen>
